{
    "name": "fortephp/forte",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "blade parser"
    ],
    "authors": [
        {
            "name": "John Koster",
            "email": "john@stillat.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10 || ^11 || ^12 || ^13",
        "illuminate/view": "^10 || ^11 || ^12 || ^13",
        "ext-dom": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0 || ^9.2 || ^10.0 || ^11.0",
        "pestphp/pest": "^2.0 || ^3",
        "laravel/pint": "^1.22",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Forte\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Forte\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Forte\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}